{
  "name": "api-clients/pusher",
  "homepage": "https://php-api-clients.org/clients/pusher",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "api-clients/command-bus": "^2.0",
    "api-clients/rx-operators": "^2.0",
    "clue/block-react": "^1.1",
    "ocramius/package-versions": "^1.1",
    "reactivex/rxphp": "^2.0",
    "rx/websocket": "^1.0"
  },
  "require-dev": {
    "api-clients/test-utilities": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "ApiClients\\Client\\Pusher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApiClients\\Tests\\Client\\Pusher\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "api-clients": {
      "command-bus": {
        "path": "src/CommandBus",
        "namespace": "ApiClients\\Client\\Pusher\\CommandBus"
      }
    }
  }
}
